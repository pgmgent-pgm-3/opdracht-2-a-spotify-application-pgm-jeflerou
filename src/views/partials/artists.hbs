<ul class="artists">
    {{#each artists}}

    <li class="artist flex-container">
        <h2>{{name}}</h2>
        <div class="button-container flex-container">
            <button class="artist-edit" data-id="{{id}}">
                <i class="fa-solid fa-pen-to-square"></i>
            </button>
            <button class="artist-delete" data-id="{{id}}">
                <i class="fa-solid fa-trash-can"></i>
            </button>
        </div>
    </li>

    {{/each}}
</ul>